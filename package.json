{
  "name": "sharedcomponents",
  "version": "0.0.1",
  "description": "Shared components to be used in HSL projects. React and styled components.",
  "main": "lib/index.js",
  "scripts": {
    "preinstall": "git config core.hooksPath hooks/",
    "build:babel": "babel src/ --out-dir lib/ --watch",
    "start": "concurrently --kill-others \"npm run build:babel\"  \"cd testServer/ && npm start\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/hsb20/sharedcomponents.git"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/hsb20/sharedcomponents#readme",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "concurrently": "^3.1.0",
    "material-ui": "^0.16.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "styled-components": "^1.4.2"
  }
}
